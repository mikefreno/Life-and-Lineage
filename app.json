{
  "expo": {
    "name": "Magic Delve",
    "slug": "magic-delve",
    "version": "0.2.2",
    "orientation": "portrait",
    "icon": "assets/images/icon.png",
    "scheme": "magicdelveapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.mikefrenodev.magicdelve",
      "buildNumber": "1",
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mikefrenodev.magicdelve",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      "expo-font",
      [
        "@sentry/react-native/expo",
        {
          "organization": "mikefreno",
          "project": "magic-delve"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.976302581992-tbi6afmid4eq5dsn63t30kpfpdhjju8m"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2307d673-d206-4c58-8170-5dc30a9ec500"
      }
    },
    "runtimeVersion": "0.2.2",
    "updates": {
      "url": "https://u.expo.dev/2307d673-d206-4c58-8170-5dc30a9ec500"
    }
  }
}
