{
  "expo": {
    "name": "Lineage",
    "slug": "life-and-lineage",
    "version": "0.6.0",
    "orientation": "portrait",
    "icon": "assets/images/icon.png",
    "scheme": "lineageapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "usesAppleSignIn": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.mikefrenodev.lineage",
      "buildNumber": "1",
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true,
        "aps-environment": "development"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.mikefrenodev.lineage",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "react-native-edge-to-edge",
      "expo-apple-authentication",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Caveat-VariableFont_wght.ttf",
            "./assets/fonts/Tangerine-Regular.ttf",
            "./assets/fonts/Tangerine-Bold.ttf",
            "./assets/fonts/PixelifySans-Regular.ttf"
          ]
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "organization": "mikefreno",
          "project": "life-and-lineage"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.976302581992-tbi6afmid4eq5dsn63t30kpfpdhjju8m"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f4a10fdc-c377-4a37-8ad7-27685b89b4d9"
      }
    },
    "runtimeVersion": "0.6.0",
    "updates": {
      "url": "https://u.expo.dev/2307d673-d206-4c58-8170-5dc30a9ec500"
    },
    "owner": "mikefrenodev"
  }
}

