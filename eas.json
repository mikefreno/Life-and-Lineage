{
  "cli": {
    "version": ">= 5.9.3",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "channel": "preview"
    },
    "production": {
      "channel": "production",
      "env": {
        "EXPO_USE_LOCAL_NODE_MODULES": "1"
      },
      "cache": {
        "key": "local-modules",
        "paths": [
          "node_modules/**/*"
        ]
      },
      "developmentClient": false,
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}

